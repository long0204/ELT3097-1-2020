<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:filterTouchesWhenObscured="false"
    android:fitsSystemWindows="false"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btchecktime2"
        android:layout_width="300dp"
        android:layout_height="72dp"
        android:layout_marginTop="236dp"
        android:background="#FDFDFD"
        android:drawableRight="@drawable/ic_baseline_more_vert_24"
        android:gravity="start"
        android:text="@string/_9_00"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.06"
        app:layout_constraintStart_toEndOf="@+id/checkBox"
        app:layout_constraintTop_toTopOf="parent"
        android:drawableEnd="@drawable/ic_baseline_more_vert_24" />

    <CheckBox
        android:id="@+id/checkBox3"
        android:layout_width="43dp"
        android:layout_height="72dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="236dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/alarmstatus"
        android:layout_width="301dp"
        android:layout_height="39dp"
        android:text="@string/no_up_coming_alarms"
        android:textColor="#050505"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.145"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.076" />

    <TextView
        android:id="@+id/comment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/next_alarm"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.046"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.022" />

    <Button
        android:id="@+id/btplu"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/hinhtron"
        android:gravity="center"
        android:text="@string/conng"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.92"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.833" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="43dp"
        android:layout_height="72dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="148dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btchecktime1"
        android:layout_width="300dp"
        android:layout_height="72dp"
        android:layout_marginTop="148dp"
        android:background="#FFFFFF"
        android:drawableRight="@drawable/ic_baseline_more_vert_24"
        android:gravity="start"
        android:text="@string/_7_00"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.06"
        app:layout_constraintStart_toEndOf="@+id/checkBox"
        app:layout_constraintTop_toTopOf="parent"
        android:drawableEnd="@drawable/ic_baseline_more_vert_24" />

    <Button
        android:id="@+id/alarmmm"
        android:layout_width="100dp"
        android:layout_height="59dp"
        android:layout_marginBottom="4dp"
        android:background="#F60B0B"
        android:drawableBottom="@drawable/ic_alarm_white_24dp"
        android:text="@string/alarm"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btsetting"
        android:layout_width="100dp"
        android:layout_height="59dp"
        android:layout_marginBottom="4dp"
        android:background="#F60B0B"
        android:drawableBottom="@drawable/ic_settings_white_24dp"
        android:text="@string/setting"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/Todaypanel" />

    <Button
        android:id="@+id/Todaypanel"
        android:layout_width="101dp"
        android:layout_height="59dp"
        android:layout_marginStart="208dp"
        android:layout_marginLeft="208dp"
        android:layout_marginBottom="4dp"
        android:background="#F60B0B"
        android:drawableBottom="@drawable/ic_art_track_white_24dp"
        android:text="@string/todaypanel"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/History"
        android:layout_width="106dp"
        android:layout_height="59dp"
        android:layout_marginStart="100dp"
        android:layout_marginLeft="100dp"
        android:layout_marginBottom="4dp"
        android:background="#F60B0B"
        android:drawableBottom="@drawable/ic_timeline_white_24dp"
        android:text="@string/history"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>